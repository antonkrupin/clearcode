distance - distance_in_km (distanceInKm)
//расстояние в километрах

baseDiscount - discount_in_percents (discountInPercents)
//размер скидки в процентах

max1 - first_maximum_number (firstMaximumNumber)
//переменная для хранения первого максимального числа
//первое название переменной подходит для очень не большой программы
//или функции, но в большой программе или через некоторое время 
//можно просто не понять что это за переменная и что в ней хранится

sumCost - result_printing_cost (resultPrintingCost)
//переменная в которой хранится "стоимость" печати строки
//заменил sum на result, так как это логичнее
//добавил printing чтобы было более понятно 

letter - letter_in_entered_string (letterInEnteredString)
//переменная для хранения буквы из введенной строки
//в начальном варианте не понятно какая буква хранится
//и откуда она берется

attackCoordsN - attack_coords_height (attackCoordsHeight)
//массив в котором хранятся координаты по высоте
//N поменял на height, так как это более понятно
//и не вызывает заблуждений

attackCoordsM - attack_coords_widht (attackCoordsWidth)
//массив в котром хранятся координаты по ширине
//M поменял на width, так как это более понятно
//и не вызывает заблуждений

sortedPrices - left_sorted_prices_list (leftSortedPricesList)
//переменная для хранения развернутого сортированного массива цен
//reversed добавил, чтоб сразу из названия было понятно
//что массив перевернутый

map1 - start_map_coords_list (startMapCoordsList)
//переменная для хранения стартовых координат карты

map2 - compare_map_coords_list (compareMapCoordsList)
//переменная для хранения координат карты для сравнения

allVotes - sum_of_all_votes (sumOfAllVotes)
//переменная для хранения суммы всех голосов

votesPercents - votes_in_percents_list (votesInPercentsList)
//массив для хранения процентов по каждому голосованию

maxVotesPercents - maximum_percentage_of_votes (maximumPercentageOfVotes)
//переменная для хранения максимального процент по голосованию
